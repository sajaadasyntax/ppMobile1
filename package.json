{
  "name": "new-expo-project",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:ios": "expo prebuild --platform ios",
    "build:android:local": "eas build --platform android --local",
    "build:ios:local": "eas build --platform ios --local",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:android:aab": "cd android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "axios": "^1.6.0",
    "expo": "~54.0.27",
    "expo-document-picker": "~14.0.8",
    "expo-linking": "~7.0.3",
    "expo-router": "~6.0.17",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "nativewind": "2.0.11",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "^4.1.0",
    "react-native-worklets": "^0.7.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "autoprefixer": "^10.4.20",
    "babel-preset-expo": "~54.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.2"
  }
}
