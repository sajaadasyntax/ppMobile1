{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-native"
    ],
    "baseUrl": "."
  },
  "include": [
    "./**/*"
  ],
  "extends": "expo/tsconfig.base"
}
